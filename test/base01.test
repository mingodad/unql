-- Basic sanity checking.
--
.new t1.db
.testcase 100
CREATE DATASET abc;
CREATE DATASET def;
INSERT INTO abc VALUE { name:"abc" };
INSERT INTO def VALUE { name:"def" };
.result

.testcase 110
SELECT FROM abc;
.result {"name":"abc"}

.testcase 120
SELECT FROM def;
.result {"name":"def"}

.testcase 130
SELECT { p:1, q:2 } FROM abc;
.result {"p":1,"q":2}
.testcase 131
SELECT {p:1, q:2+3} FROM abc;
.result {"p":1,"q":5}
.testcase 132
SELECT {p:1, q:{x:[1,2,3],y:11}} FROM abc;
.result {"p":1,"q":{"x":[1,2,3],"y":11}}
